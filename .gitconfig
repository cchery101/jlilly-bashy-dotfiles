[core]
  excludesfile = /Users/jlilly/.gitignore 
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[user]
  name = Justin Lilly
  email = justin@justinlilly.com
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  log1 = log --pretty=oneline --abbrev-commit
  serve = daemon --reuseaddr --base-path=. --export-all --verbose
[merge]
  tool=kdiff3
